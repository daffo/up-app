{
  "name": "up-app",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:contracts": "jest --config jest.contracts.config.js",
    "test:e2e": "npm run test:e2e:guest && npm run test:e2e:auth",
    "test:e2e:guest": "maestro test __tests__/e2e/guest-happy-path.yaml",
    "test:e2e:auth": "source .env && maestro test -e E2E_TEST_EMAIL=\"$E2E_TEST_EMAIL\" -e E2E_TEST_PASSWORD=\"$E2E_TEST_PASSWORD\" __tests__/e2e/auth-happy-path.yaml"
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.3",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-navigation/native": "^7.1.22",
    "@react-navigation/native-stack": "^7.8.3",
    "@supabase/supabase-js": "^2.86.0",
    "expo": "~54.0.33",
    "expo-auth-session": "~7.0.10",
    "expo-crypto": "~15.0.8",
    "expo-dev-client": "~6.0.20",
    "expo-font": "~14.0.11",
    "expo-image": "~3.0.11",
    "expo-image-manipulator": "~14.0.8",
    "expo-linking": "~8.0.11",
    "expo-localization": "^17.0.8",
    "expo-status-bar": "~3.0.9",
    "expo-web-browser": "~15.0.10",
    "i18next": "^25.8.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-i18next": "^16.5.4",
    "react-native": "0.81.5",
    "react-native-draggable-flatlist": "^4.0.3",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-image-pan-zoom": "^2.1.12",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "15.12.1",
    "react-native-url-polyfill": "^3.0.0",
    "react-native-web": "^0.21.0",
    "react-native-worklets": "0.5.1",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@types/jest": "29.5.14",
    "@types/react": "~19.1.0",
    "babel-preset-expo": "~54.0.9",
    "baseline-browser-mapping": "^2.9.19",
    "dotenv": "^17.2.3",
    "jest": "^29.7.0",
    "jest-expo": "~54.0.17",
    "typescript": "~5.9.2"
  },
  "private": true
}
