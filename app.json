{
  "expo": {
    "name": "up-app",
    "slug": "up-app",
    "scheme": "upapp",
    "version": "0.2.3-alpha",
    "orientation": "default",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.daffo.upapp"
    },
    "android": {
      "package": "com.daffo.upapp",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "viewport": "width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no"
    },
    "extra": {
      "eas": {
        "projectId": "3f8a144e-b45c-46a7-85d0-00cd453ad508"
      }
    },
    "owner": "daffo",
    "plugins": [
      "expo-web-browser",
      "expo-font"
    ]
  }
}
